<div class="row">
  <div class="col-12">
    <div class="card">
      <!-- /.card-header -->
      <div class="card-body table-responsive p-0" style="height: 300px;">
        <table class="table table-head-fixed text-nowrap table-bordered">
          <thead>
            <tr>
              <th>Type</th>
              <th>Log</th>
              <th>Added Time</th>
            </tr>
          </thead>
          <tbody>
            <% if (customerlog.length > 0){%>
              <%  for (let customerL of customerlog){ %>
            <tr>
              <td><%= customerL.type %></td>
              <td><%= customerL.log %></td>
              <td><%= moment(customerL.addedTime).format('DD/MM/YYYY h:mm:ss a'); %></td>
            </tr>
          <% } } else {%>
            <tr>
              <td colspan="3">No data found</td>
            </tr>
            <% } %>
          </tbody>
        </table>
      </div>
      <!-- /.card-body -->
    </div>
    <!-- /.card -->
  </div>
</div>